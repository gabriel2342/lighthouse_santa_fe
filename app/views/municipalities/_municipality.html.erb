
<%= turbo_frame_tag municipality do %>
  <div id="<%= dom_id municipality %>">
    <div class="row">
      <div class="col">
        <%= link_to municipality.name, organization_municipality_path(@organization, municipality), data: {turbo_frame: "_top"} %>
      </div> 
      <div class="col d-flex inline align-items-center">
        <%= link_to 'Edit', edit_organization_municipality_path(@organization, municipality), class: "btn btn-secondary mx-1"%>  
        <%= button_to "Delete", organization_municipality_path(municipality), method: :delete, form: { data: { turbo_confirm: 'Are you sure?' } }, class: "btn btn-danger mx-1"%>
      </div>
    </div>
  </div>
<% end %>
