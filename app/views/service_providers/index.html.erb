<p style="color: green"><%= notice %></p>

<div class="container">
  <h1>Service providers</h1>

  <div id="service_providers">
    <% @service_providers.each do |service_provider| %>
      <%= render service_provider %>
      <p>
        
          <%= link_to "View", [@organization, @municipality, service_provider] %> | 
          <%= link_to "Edit", edit_organization_municipality_service_provider_path(@organization, @municipality, service_provider) %> | 
          <%= link_to "Delete", [@organization, @municipality, service_provider], data: {"turbo-method": :delete } %>
        
      </p>
    <% end %>
  </div>
</div>

