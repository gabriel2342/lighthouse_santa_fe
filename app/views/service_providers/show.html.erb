<div class="container d-flex justify-content-center mt-3">

  <p style="color: white"><%= notice %></p>

  <%= render @service_provider %>
  <div class="container d-flex justify-content-center" >
              <%= link_to "Edit", edit_organization_municipality_service_provider_path(@organization, @municipality,@service_provider), class: "btn btn-primary"%> |
              <%= link_to "Delete", [@organization, @municipality, @service_provider], data: {"turbo-method": :delete }, class: "btn btn-danger" %>
  </div>
  <div class="container d-flex justify-content-center" >
    <%= link_to "Back to Municipality", organization_municipality_path(@organization,@municipality), class: "btn btn-secondary"%>
  </div>  
</div>

